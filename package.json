{
  "name": "bandcamp-collector",
  "displayName": "Bandcamp Collector",
  "version": "0.0.1",
  "description": "[description]",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite build --watch",
    "dev:js": "run-p dev:js:*",
    "dev:js:deps": "rollup --config rollup.config.content.deps.ts --configPlugin typescript --watch",
    "dev:js:contentScript": "yarn dev-build:js:contentScript && chokidar \"src/contentScripts/*\" -c \"yarn dev-build:js:contentScript\"",
    "dev-build:js:contentScript": "babel --extensions \".js,.jsx,.ts,.tsx\" --config-file ./babel.config.content.json ./src/contentScripts --out-dir ./extension/dist/contentScripts",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.* && mkdir extension/dist && mkdir extension/dist/contentScripts",
    "lint": "eslint 'src/**/*.{json,ts,js,vue}'"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@ffflorian/jszip-cli": "^3.1.5",
    "@iconify/json": "^1.1.408",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.10.2",
    "@types/webextension-polyfill": "^0.8.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "chokidar": "^3.5.2",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^7.32.0",
    "esno": "^0.10.0",
    "fs-extra": "^10.0.0",
    "kolorist": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.5.11",
    "vite": "^2.6.2",
    "web-ext": "^6.4.0",
    "webextension-polyfill": "^0.8.0"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/base": "^5.0.0-alpha.69",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "clsx": "^1.1.1",
    "framer-motion": "^6.2.6",
    "fuse.js": "^6.5.3",
    "niue": "^0.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webext-bridge": "^5.0.3"
  }
}
